{
  "expo": {
    "name": "Quiz",
    "slug": "Quiz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/Quiz-App-Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#135D66"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/Quiz-App-Logo.png",
        "backgroundColor": "#135D66"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/icon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            {
              "asset": "./assets/fonts/Lato-Black.ttf",
              "postscriptName": "Lato-Black"
            },
            {
              "asset": "./assets/fonts/Lato-Bold.ttf",
              "postscriptName": "Lato-Bold"
            },
            {
              "asset": "./assets/fonts/Lato-Light.ttf",
              "postscriptName": "Lato-Light"
            },
            {
              "asset": "./assets/fonts/Lato-Regular.ttf",
              "postscriptName": "Lato-Regular"
            },
            {
              "asset": "./assets/fonts/Lato-Thin.ttf",
              "postscriptName": "Lato-Thin"
            }
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
